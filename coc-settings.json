{
  "eslint.nodePath": "./node_modules",
  "eslint.autoFixOnSave": true,
  "prettier.disableSuccessMessage": true,
  "coc.preferences.formatOnType": false,
  "suggest.timeout": 1000,
  "coc.preferences.formatOnSaveFiletypes": [
    "dart",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "json",
    "css",
    "scss",
    "html",
    "ruby",
    "yaml",
    "ex",
    "leex",
    "heex"
  ],
  "tailwindCSS.jsLanguages": [
    "javascript",
    "javascriptreact",
    "reason",
    "typescriptreact"
  ],
  "tailwindCSS.cssLanguages": [
    "css",
    "less",
    "postcss",
    "sass",
    "scss",
    "stylus",
    "vue"
  ],
  "tailwindCSS.includeLanguages": {
    "plaintext": "html"
  },
  "json.format.enable": true,
  "elixir.pathToElixirLS": "~/.elixir-ls/release/language_server.sh",
  "elixirLS.suggestSpecs": false,
  "elixirLS.dialyzerEnabled": false,
  "elixirLS.fetchDeps": false,
  "tailwindCSS.trace.server": "verbose",
  "tsserver.formatOnType": true,
  "diagnostic.errorSign": ">>",
  "diagnostic.warningSign": "•",
  "diagnostic.infoSign": "•",
  "list.source.grep.command": "rg",
  "list.source.grep.excludePatterns": ["node_modules"],
  "suggest.enablePreview": true,
  "tsserver.log": "verbose",
  "flutter.lsp.initialization.closingLabels": true,
  "flutter.provider.hot-reload": true,
  "html.autoClosingTags": true
}
